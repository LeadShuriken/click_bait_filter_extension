{
  "manifest_version": 2,
  "name": "Click Bait Filter Extension",
  "version": "0.1",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "webRequest",
    "storage",
    "history",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "webpack.16.png",
    "48": "webpack.48.png",
    "128": "webpack.128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "webpack.16.png",
      "24": "webpack.48.png",
      "32": "webpack.128.png"
    },
    "default_popup": "popup.html",
    "default_title": "Chrome Clickbait Filter"
  },
  "web_accessible_resources": [
    "blocked.png"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content-script.js"
      ],
      "css": [
        "style.css"
      ]
    }
  ]
}